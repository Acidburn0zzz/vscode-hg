        {
          "command": "hg.markResolved",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == conflict",
          "group": "0_resolution"
        },
        {
          "command": "hg.openFile",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == conflict",
          "group": "navigation"
        },
        {
          "command": "hg.openFile",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == merge",
          "group": "navigation"
        },
        {
          "command": "hg.unresolve",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == merge",
          "group": "1_modification"
        },
        {
          "command": "hg.openChange",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == staging",
          "group": "navigation"
        },
        {
          "command": "hg.openFile",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == staging",
          "group": "navigation"
        },
        {
          "command": "hg.unstage",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == staging",
          "group": "1_modification"
        },
        {
          "command": "hg.unstage",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == staging",
          "group": "inline"
        },
        {
          "command": "hg.openChange",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == working",
          "group": "navigation"
        },
        {
          "command": "hg.openFile",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == working",
          "group": "navigation"
        },
        {
          "command": "hg.stage",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == working",
          "group": "1_modification"
        },
        {
          "command": "hg.clean",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == working",
          "group": "1_modification"
        },
        {
          "command": "hg.clean",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == working",
          "group": "inline"
        },
        {
          "command": "hg.stage",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == working",
          "group": "inline"
        },
        {
          "command": "hg.add",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == untracked",
          "group": "inline"
        },
        {
          "command": "hg.add",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == untracked",
          "group": "1_modification"
        },
        {
          "command": "hg.openFile",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == untracked",
          "group": "navigation"
        },
        {
          "command": "hg.forget",
          "when": "config.hg.enabled && scmProvider == hg && hgState == idle && scmResourceGroup == working",
          "group": "2_modification"
        }
      ],
      "editor/title": [
        {
          "command": "hg.openFileFromUri",
          "group": "navigation",
          "when": "config.hg.enabled && scmProvider == hg && isInDiffEditor && resourceScheme != extension"
        },
        {
          "command": "hg.openChangeFromUri",
          "group": "navigation",
          "when": "config.hg.enabled && scmProvider == hg && !isInDiffEditor && resourceScheme != extension"
        },
        {
          "command": "hg.fileLog",
          "when": "config.hg.enabled && scmProvider == hg && !isInDiffEditor"
        }
      ]
    },
    "configuration": {
      "title": "Hg (Mercurial)",
      "properties": {
        "hg.enabled": {
          "type": "boolean",
          "description": "%config.enabled%",
          "default": true
        },
        "hg.path": {
          "type": [
            "string",
            "null"
          ],
          "description": "%config.path%",
          "default": null,
          "isExecutable": true
        },
        "hg.autorefresh": {
          "type": "boolean",
          "description": "%config.autorefresh%",
          "default": true
        },
        "hg.commandMode": {
          "type": "string",
          "description": "%config.commandMode%",
          "default": "cmdline",
          "enum": [
            "cmdline",
            "server"
          ]
        },
        "hg.autoinout": {
          "type": "boolean",
          "description": "%config.autoinout%",
          "default": true
        },
        "hg.countBadge": {
          "type": "string",
          "enum": [
            "all",
            "tracked",
            "off"
          ],
          "description": "%config.countBadge%",
          "default": "all"
        },
        "hg.allowPushNewBranches": {
          "type": "boolean",
          "description": "%config.allowPushNewBranches%",
          "default": false
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "vscode-nls": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.4",
    "vscode": "^1.1.0"
  }
}
