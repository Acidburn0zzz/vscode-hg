      "editor/title": [
        {
          "command": "hg.openFileFromUri",
          "group": "navigation",
          "when": "config.hg.enabled && scmProvider == hg && isInDiffEditor && resourceScheme != extension"
        },
        {
          "command": "hg.openChangeFromUri",
          "group": "navigation",
          "when": "config.hg.enabled && scmProvider == hg && !isInDiffEditor && resourceScheme != extension"
