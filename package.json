    "configuration": {
      "title": "Mercurial",
      "properties": {
        "hg.enabled": {
          "type": "boolean",
          "description": "%config.enabled%",
          "default": true
        },
        "hg.path": {
          "type": [
            "string",
            "null"
          ],
          "description": "%config.path%",
          "default": null,
          "isExecutable": true
        },
        "hg.autorefresh": {
          "type": "boolean",
          "description": "%config.autorefresh%",
          "default": true
        },
        "hg.autoincoming": {
          "type": "boolean",
          "description": "%config.autoincoming%",
          "default": true
        },
        "hg.confirmSync": {
          "type": "boolean",
          "description": "%config.confirmSync%",
          "default": true
        },
        "hg.countBadge": {
          "type": "string",
          "enum": [
            "all",
            "tracked",
            "off"
          ],
          "description": "%config.countBadge%",
          "default": "all"
        },
        "hg.updateType": {
          "type": "string",
          "enum": [
            "all",
            "local",
            "tags",
            "remote"
          ],
          "description": "%config.updateType%",
          "default": "all"
        }
      }
